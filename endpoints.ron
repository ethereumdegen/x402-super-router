(
  endpoints: [
    (
      path: "/generate_image",
      fal_model: "fal-ai/flux/schnell",
      cost: "1000000000000000000000",
      description: "Generate an AI image (1000 STARKBOT)",
      response_url_path: "images.0.url",
      request_params: {
        "num_inference_steps": 4,
        "image_size": "square",
        "num_images": 1,
        "output_format": "png",
        "enable_safety_checker": true,
      },
      default_prompt: "a fun colorful surreal meme illustration",
      media_type: "image",
      output_extension: "png",
      cache_dir: "public/images",
      static_serve_path: "/images",
      post_process: None,
    ),
    (
      path: "/generate_gif",
      fal_model: "fal-ai/fast-animatediff/turbo/text-to-video",
      cost: "1000000000000000000000",
      description: "Generate an animated GIF (1000 STARKBOT)",
      response_url_path: "video.url",
      request_params: {
        "negative_prompt": "(bad quality, worst quality:1.2), ugly, blurry",
        "num_frames": 16,
        "num_inference_steps": 8,
        "guidance_scale": 2.0,
        "fps": 8,
        "video_size": "square",
      },
      default_prompt: "a fun random weird surreal animated meme",
      media_type: "gif",
      output_extension: "gif",
      cache_dir: "public/gifs",
      static_serve_path: "/gifs",
      post_process: FfmpegToGif(
        input_extension: "mp4",
        ffmpeg_args: [
          "-vf",
          "fps=10,scale=512:-1:flags=lanczos,split[s0][s1];[s0]palettegen[p];[s1][p]paletteuse",
          "-loop",
          "0",
        ],
      ),
    ),
    (
      path: "/generate_kling",
      fal_model: "fal-ai/kling-image/o3/text-to-image",
      cost: "1000000000000000000000",
      description: "Generate a Kling image (1000 STARKBOT)",
      response_url_path: "images.0.url",
      request_params: {
        "aspect_ratio": "1:1",
      },
      default_prompt: "a fun colorful surreal meme illustration",
      media_type: "image",
      output_extension: "png",
      cache_dir: "public/kling",
      static_serve_path: "/kling",
      post_process: None,
    ),
  ],
)
